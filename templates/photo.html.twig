{% extends 'base.html.twig' %}
{% block title %}Fotos de nuestros gatos{% endblock %}
{% block stylesheets %}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/styles.css" />
{% endblock %}
{% block body %}
    <h1>Nueva foto</h1>
    {{ form_start(formulario) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(formulario) }}
    </div>
    <div class="col">
        <div class="col" id="form-fileName">
            {{ form_row(formulario.fileName) }}
        </div>
        <div class="col" id="form-description">
            {{ form_row(formulario.description) }}
        </div>
        <div class="row">
            <div class="col" id="form-save">
                {{ form_row(formulario.save) }}
            </div>
            <div class="col" id="form-description">
                <a href="/" class="btn btn-primary">Volver</a>
            </div>
        </div>
    </div>
{{ form_end(formulario) }}
{% endblock %}